{
  "name": "periodic-table",
  "version": "1.0.0",
  "description": "An AngularJS, ES6, Webpack, Node API example project creating a periodic table",
  "scripts": {
    "start": "npm run api & npm run dev",
    "build": "webpack --config webpack.build.js --bail -p",
    "dev": "node node_modules/.bin/webpack-dev-server --history-api-fallback --hot --inline --progress",
    "api": "nodemon ./api/",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanpapaleo/periodic-table.git"
  },
  "author": "Jordan Papaleo",
  "dependencies": {
    "angular": "^1.3.16",
    "angular-ui-router": "^0.2.15",
    "autoprefixer-core": "^5.2.0",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^5.5.8",
    "body-parser": "^1.13.3",
    "bootstrap": "^3.3.5",
    "connect": "^3.4.0",
    "connect-send-json": "^1.0.0",
    "css-loader": "^0.14.5",
    "es6-promise": "^2.3.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "gsap": "^1.17.0",
    "html-webpack-plugin": "^1.5.2",
    "http": "0.0.0",
    "json-loader": "^0.5.2",
    "lodash": "^3.9.1",
    "node-libs-browser": "^0.5.2",
    "postcss-loader": "^0.4.4",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.1",
    "babel-eslint": "^3.1.20",
    "eslint": "^0.24.0",
    "eslint-loader": "^0.14.1",
    "isparta-instrumenter-loader": "^0.2.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.36",
    "karma-coverage": "^0.4.2",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.19",
    "karma-webpack": "^1.5.1",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.17"
  }
